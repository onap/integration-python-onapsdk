{
    "requestDetails": {
        "requestInfo": {
            "instanceName": "{{ service_instance.instance_name }}",
            "source": "VID",
            "suppressRollback": false,
            "requestorId": "demo",
            "productFamilyId": "1234"
        },
        "modelInfo": {
            "modelType": "pnf",
            "modelInvariantId": "{{ pnf.model_invariant_id }}",
