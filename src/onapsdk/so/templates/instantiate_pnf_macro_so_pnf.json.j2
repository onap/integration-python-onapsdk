            "modelName": "{{ pnf.model_name }}",
            "modelVersion": "{{ pnf.model_version }}",
            "modelCustomizationId": "{{ pnf.model_customization_id }}",
            "modelInstanceName": "{{ pnf.name }}"
        },
        "cloudConfiguration": {
